  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.0.0",
    "@types/jest": "^27.0.0",
    "babel-loader": "^8.1.0",
    "bundlesize": "^0.18.0",
    "jest": "^27.0.0",
    "react": "^17.0.1",
    "ts-jest": "^27.0.0"
  }
