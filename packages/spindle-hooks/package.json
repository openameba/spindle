{
  "name": "@openameba/spindle-hooks",
  "version": "0.0.1",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "test": "run-p bundlesize test:interaction",
    "test:interaction": "jest",
    "prebundlesize": "yarn build:script",
